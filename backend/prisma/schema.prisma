generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test {
  id         BigInt    @id(map: "Tickets_duplicate_pkey") @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  tickets    Json
}

model Tickets {
  id         BigInt    @id @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  tickets    Json
  lotto      String
  buyer      String
  signature  String
  address    String
}


model Product {
  id          BigInt    @id @default(autoincrement())
  name       String
  active    Boolean
  description String
  images      String[]
  maxSupply   Int?
  tags      String[]
  prices      Json[]
  meta    Json
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}